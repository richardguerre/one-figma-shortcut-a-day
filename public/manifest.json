{
  "manifest_version": 2,
  "name": "One Figma Shortcut a Day",
  "version": "0.1.0",
  "description": "Learn a Figma shortcut everyday, and become a Figma Master.",
  "homepage_url": "https://github.com/richardguerre/one-figma-shortcut-a-day",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Richard Guerre",
  "content_scripts": [
    {
      "matches": ["*://*.figma.com/*"],
      "css": ["static/css/main.css"],
      "js": ["static/js/runtime-main.js", "static/js/2.js", "static/js/main.js"]
    }
  ],
  "permissions": ["storage"]
}
